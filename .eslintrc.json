{
	"extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
	"plugins": ["react", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"ignorePatterns": [".github/*", "public/*"],
	"rules": {
		"no-empty-pattern": "warn",
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
	}
}
